<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFrameworks>netstandard2.1;netstandard2.0</TargetFrameworks>
        <PackageDescription>Provides Autofac support for a Rocket Surgery Application</PackageDescription>
        <PackageTags>$(PackageTags);autofac</PackageTags>
    </PropertyGroup>
    <ItemGroup>
        <PackageReference Include="Autofac.Extensions.DependencyInjection" />
        <PackageReference Include="Microsoft.Extensions.DependencyInjection" />
        <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" />
        <PackageReference Include="Rocket.Surgery.Conventions" />
        <PackageReference Include="System.Diagnostics.DiagnosticSource" />
    </ItemGroup>
    <ItemGroup>
      <ProjectReference Include="..\Autofac.Conventions.Abstractions\Rocket.Surgery.Conventions.Autofac.Abstractions.csproj" />
      <ProjectReference Include="..\Autofac.Extensions\Rocket.Surgery.Extensions.Autofac.Extensions.csproj" />
    </ItemGroup>
</Project>
